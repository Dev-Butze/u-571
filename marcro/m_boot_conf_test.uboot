# m_boot_conf_test
#
# test for m_boot_conf 
 
run m_load_env;
run m_run_bootscr;
run m_set_bootargs;
if run m_load_kernel && run m_load_initrd && run m_load_fdt; then
  if test ${kernel} = zImage; then
    echo bootz ${krnl_addr} ${initrd_addr} ${fdt_addr};
  else
    echo bootm ${krnl_addr} ${initrd_addr} ${fdt_addr};
  fi;
else
  false;
fi;
