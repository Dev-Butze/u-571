# m_init_board
# initialize board dependant u-boot settings
#

# CuBox-i
if test ${ftd} = "auto"; then
  setenv ftd ...;
  setenv kernel zImage;
  setenv krnl_addr 0x10800000;
  setenv fdt_addr 0x18000000;
  setenv fdt_high 0xffffffff;
  setenv initrd_adr -;
  setenv initrd_high 0xffffffff;
fi
# vim:ft=sh
