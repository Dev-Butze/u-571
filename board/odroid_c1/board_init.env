baudrate	115200
board		odroid-c1
bootcmd		run m_autoboot;
bootenv		uEnv.txt
bootfile	boot.img
bootdelay	5
bootm_low	0x00000000
bootm_size	0x80000000
bootscr		boot.scr
console         ttyS0
cpu		s805
dbglevel	0
errlevel	0
ethact		Meson_Ethernet
fdt             meson8b_odroidc.dtb
fdt_addr	0x21800000
fdt_high	0xffffffff
initrd		uInitrd
initrd_addr	0x22000000
initrd_high	0x60000000
kernel		uImage
krnl_addr	0x21000000
mmcdev		0
prefix		/boot/conf.d
satadev		0
stderr		serial,vga
stdin		serial,usbkbd
stdout		serial,vga
usbdev		0
m_bc_m0p1	setenv mmcdev 0:1; run m_bc_default_mmc;
m_bc_m0p2	setenv mmcdev 0:2; run m_bc_default_mmc;
m_bc_m1p1	setenv mmcdev 1:1; run m_bc_default_mmc;
m_bc_u0p1	p_prefix=${prefix}; p_conf=u0p1; if run m_init_mmc; then run m_boot_conf; fi; false;
